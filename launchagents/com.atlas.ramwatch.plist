<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0"><dict>
  <key>Label</key><string>com.atlas.ramwatch</string>
  <key>ProgramArguments</key>
  <array>
    <string>/bin/bash</string>
    <string>-lc</string>
    <string>$HOME/.atlas/ramwatch/ramwatch.sh --loop</string>
  </array>
  <key>RunAtLoad</key><true/>
  <key>KeepAlive</key><true/>
  <key>StartInterval</key><integer>15</integer>
  <key>StandardOutPath</key><string>$HOME/.atlas/ramwatch/logs/ramwatch.log</string>
  <key>StandardErrorPath</key><string>$HOME/.atlas/ramwatch/logs/ramwatch.err</string>
</dict></plist>
